<resources>
    <declare-styleable name="AnimButton">
        <attr name="recording" format="reference" />
        <attr name="typing" format="reference" />
        <attr name="duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="DevlomiSeekbar">
        <attr name="progressColor" format="color" />
        <attr name="thumbColor" format="color" />
    </declare-styleable>


    <declare-styleable name="LinearLayoutWithBackgroundTint">
        <attr name="linearBgTintColor" format="color" />
    </declare-styleable>


    <declare-styleable name="RelativeLayoutWithBackgroundTint">
        <attr name="relativeBgTintColor" format="color" />
    </declare-styleable>

    <declare-styleable name="FrameLayoutWithBackgroundTint">
        <attr name="frameBgTintColor" format="color" />
    </declare-styleable>


    <declare-styleable name="TextViewDrawableCompat">
        <attr name="drawableStartCompat" format="reference" />
        <attr name="drawableEndCompat" format="reference" />
        <attr name="drawableTopCompat" format="reference" />
        <attr name="drawableBottomCompat" format="reference" />
        <attr name="drawableTintCompat" format="color" />
    </declare-styleable>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>


    <declare-styleable name="BubbleToggleView">
        <attr name="bt_icon" format="reference" />
        <attr name="bt_iconWidth" format="dimension" />
        <attr name="bt_iconHeight" format="dimension" />
        <attr name="bt_shape" format="reference" />
        <attr name="bt_shapeColor" format="color" />
        <attr name="bt_showShapeAlways" format="boolean" />
        <attr name="bt_title" format="string" />
        <attr name="bt_titleSize" format="dimension" />
        <attr name="bt_colorActive" format="color" />
        <attr name="bt_colorInactive" format="color" />
        <attr name="bt_active" format="boolean" />
        <attr name="bt_duration" format="integer" />
        <attr name="bt_padding" format="dimension" />
        <attr name="bt_titlePadding" format="dimension" />
        <attr name="bt_badgeTextSize" format="dimension" />
        <attr name="bt_badgeBackgroundColor" format="color" />
        <attr name="bt_badgeTextColor" format="color" />
        <attr name="bt_badgeText" format="string" />
    </declare-styleable>
    <declare-styleable name="BubbleNavigationConstraintView">
        <attr name="bnc_mode">
            <enum name="spread" value="0" />
            <enum name="inside" value="1" />
            <enum name="packed" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LavaFab">
        <attr name="lavaParentIcon" format="reference"/>
        <attr name="lavaLeftIcon" format="reference"/>
        <attr name="lavaLeftTopIcon" format="reference"/>
        <attr name="lavaTopIcon" format="reference"/>
        <attr name="lavaRightTopIcon" format="reference"/>
        <attr name="lavaRightIcon" format="reference"/>
        <attr name="lavaRightBottomIcon" format="reference"/>
        <attr name="lavaBottomIcon" format="reference"/>
        <attr name="lavaLeftBottomIcon" format="reference"/>
        <attr name="lavaParentSize" format="dimension"/>
        <attr name="lavaBackgroundColor" format="color"/>
        <attr name="lavaDrawShadow" format="boolean"/>
        <attr name="lavaChild" format="flags">
            <flag name="top" value="1" />
            <flag name="right" value="2" />
            <flag name="bottom" value="4" />
            <flag name="left" value="8" />
            <flag name="leftTop" value="16" />
            <flag name="rightTop" value="32" />
            <flag name="rightBottom" value="64" />
            <flag name="leftBottom" value="128" />
            <flag name="none" value="0" />
            <flag name="all" value="255" />
        </attr>
    </declare-styleable>
</resources>